[push]
	default = matching
[alias]
  co = checkout
  com = checkout master
  ci = commit
  cm = commit -v
  t = status
  s = status
  st = status
  br = branch --sort=-committerdate
  df = diff --color
  dfom = diff master --color
  pl = pull
  p = push origin HEAD
  ad = add --patch
  rb = rebase origin/master
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  su = submodule update --init --recursive
  pim = pull origin master
  rbm = pull --rebase origin master
  d = difftool
  lc = log -n 1
[core]
  editor  = nvim
  autocrlf = input
[color]
  ui = auto
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[branch "master"]
  remote = origin
  merge = refs/heads/master
[credential]
  helper = osxkeychain
[init]
  templatedir = ~/.git_template
[user]
	name = Gedward Gonzalez
	email = gonzalezgedward@gmail.com
[advice]
	ignoredHook = false
